{
	"manifest_version": 2,
	"name": "passman",
	"version": "1.0",

	"description": "WebExtension for password manager!",

	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["passman.js"]
		}
	],

	"background": {
		"scripts": ["handler.js"]
	},

	"permissions": [
		"http://localhost:13880/"
	]
}